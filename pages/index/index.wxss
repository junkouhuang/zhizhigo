page {
  background: #f5f5f5;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.swiper-container {
  position: relative;
  margin-bottom:6rpx;
}

.swiperitem {
  overflow: hidden;
  background-repeat: no-repeat;
  background-position: center center;
  width: 640rpx !important;
  height: 400rpx !important;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
  margin: 0;
}

.swiper {
  background-color: #fff;
}

.swiper-tab {
  background-color: #fff;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
}

swiper-item {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.swiper-tab-list {
  float: left;
  font-size: 30rpx;
  height: 92rpx;
  line-height: 92rpx;
  width: 33%;
  text-align: center;
}

.nav {
  height: 92rpx;
  line-height: 92rpx;
  width: 100%;
  display: block;
  text-align: center;
  font-weight: bold;
  position: relative;
}

.swiper-tab-list text {
  width: 100%;
  display: block;
  height: 40rpx;
  line-height: 40rpx;
}
.swiper-tab-list span {
  width:40rpx;
  height:40rpx;
  position:absolute;
  top:0;
  right:0;
}

.on {
  border-bottom: 3rpx solid #53a24f;
  font-weight: bold;
  width: 25%;
  margin: 6rpx auto 0;
}

.colr1 {
  color: red;
  position: relative;
}
.colr2 {
  color: #53a24f;
}


.dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}

.dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 8rpx;
  transition: all 0.6s;
}

.dots .dot.active {
  width: 24rpx;
  /** background: #f80;**/
  background: #53a24f;
}

.template {
  display: flex;
  flex-direction: row;
  flex-flow: row wrap;
  width: 99%;
  padding-right: 1%;
  height: 100%;
}

.prolist {
  width: 50%;
}

.template-row {
  width: 100%;
  margin: 7rpx 0;
}

.msg {
  text-align: center;
  width: 200rpx;
  margin: 0 auto;
  font-size: 30rpx;
  margin-top: 50rpx;
}

.flex {
  position: fixed;
  animation: down 1s 1;
  z-index: 99;
  top: 0;
}
@keyframes down {
	from {top:-120rpx;}
	to {top:0}
}

.com-widget-goTop {
  position: fixed;
  bottom: -110rpx;
  right: 10rpx;
  border-radius: 50%;
  overflow: hidden;
}

.com-widget-goTop .icon-gotop {
  background-color: #333;
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  line-height: 136rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  border-radius: 50%;
  opacity: 0.7;
}

.com-widget-goTop  image {
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  line-height:138rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  opacity: 0.7;
}

.loadingmore {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  flex-flow: row;
  display: -webkit-flex;
  -webkit-flex-flow: row wrap;
}

.refresh {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  flex-flow: row;
  display: -webkit-flex;
  -webkit-flex-flow: row wrap;
}

.loadingmore_img {
  width: 40%;
  text-align: right;
}

.loadingmore_img image {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: 3rpx;
  right: 10rpx;
}

.loadingmore_text {
  width: 60%;
  text-align: left;
}

.loadingmore_text text {
  position: relative;
  top: -6rpx;
}

.refresh_text {
  width: 100%;
  text-align: text;
}

.refresh_text text {
  position: relative;
  top: -6rpx;
}

.refresh_text button {
  text-decoration: underline;
  color: blueviolet;
  position: relative;
  bottom: 6rpx;
  display: inline;
  font-size: 28rpx;
  padding-left: 10rpx;
}

.refresh_text button::after {
  border: none;
}

.loadingmore image :before {
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}

.loadingmore.complete:before {
  display: none;
}

.complete {
  width: 100%;
  height: 100rpx;
  text-align: center;
  font-size: 30rpx;
}

.complete image {
  width: 50rpx;
  height: 50rpx;
  margin-top: 14rpx;
}

.complete_all {
  width: 100%;
  display: block;
}


.complete image :before {
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}

.complete.complete:before {
  display: none;
}

.video-row-in {
  display: flex;
  flex-direction: column;
  margin-left: 7rpx;
  background: #fff;
  position: relative;
}

.video-row-image {
  width: 100%;
  display: flex;
  overflow: hidden;
  float: left;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  position: relative;
}

.fbTime {
  width: 100%;
  height: 50rpx;
  background: #333;
  opacity: 0.8;
  position: absolute;
  bottom: 0;
  font-size: 25rpx;
  color: #fff;
  text-align: center;
  line-height: 50rpx;
}

.video-row-in-sq {
  width: 128rpx;
  height: 90rpx;
  position: absolute;
  top: 0;
}

.video-row-in-img {
  border-radius: 6rpx;
  width: 100%;
  height: 16rpx;
  float: left;
  margin: 0 auto;
}

.video-new {
  width: 64rpx;
  height: 26rpx;
  position: relative;
  display: block;
}

.one-tag {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 10rpx;
}

.one-tag text {
  font-family: "微软雅黑";
  overflow: hidden; /*自动隐藏文字*/
  text-overflow: ellipsis; /*文字隐藏后添加省略号*/
  white-space: nowrap; /*强制不换行*/
}

.one-tag-title {
  font-size: 31rpx;
  padding: 10rpx 6rpx 2rpx 6rpx;
  font-weight: bold;
}

.one-tag-sellprice {
  font-size: 31rpx;
  color: #53a24f;
  font-weight: bold;
}

.one-tag-sellprice-txt {
  font-size: 28rpx;
  font-weight: bold;
}

.move_up {
  animation: mymove 1s 1;
  animation-fill-mode: forwards;
}

@keyframes mymove {
  from {
    bottom: -110rpx;
  }

  to {
    bottom: 200rpx;
  }
}

.move_down {
  animation: mymove2 1s 1;
  animation-fill-mode: forwards;
}

@keyframes mymove2 {
  from {
    bottom: 200rpx;
  }

  to {
    bottom: -110rpx;
  }
}

.one-tag-addToCartAndTime {
  display: flex;
  flex-direction: row;
  width: 98%;
  padding-right: 2%;
  flex-flow: row wrap;
}

.one-tag-addToCartImg {
  position: absolute;
  top: -18rpx;
}

.one-tag-time {
  font-size: 26rpx;
  color: #a1a1a1;
  width: 70%;
  height: 60rpx;
  margin-left: 5rpx;
  line-height: 60rpx;
}

.one-tag-time text {
  background: #edf7ed;
  color: #53a24f;
  border-radius: 10rpx;
  font-size: 30rpx;
}

.one-tag-addToCart {
  width: 26%;
  height: 50rpx;
  position: relative;
}

.no_network {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #fff;
}

.no_network view {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #ccc;
  position: absolute;
  top: 10%;
}

.no_network text {
  padding: 2rpx 8rpx;
  border: 2rpx solid #ccc;
  border-radius: 24rpx;
  width: 66rpx;
  margin-left: 8rpx;
}

.bj {
  background: #fff;
  width: 100%;
  height: 100%;
  background: #fff;
  position: fixed;
  top: 0;
}

.search {
  width:90%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  flex-flow: row nowrap;
  position: absolute;
  top: 10rpx;
  z-index: 999;
  margin-left: 5%;
}

.search-panel {
  width:87%;
  height:60rpx;
  background-color:#f7f7f7;
  border-radius:50rpx;
  display:flex;
  flex-direction:row;
  flex-flow:row nowrap;
  border:2rpx solid #ededed;
  margin-left:3%;
  margin-top:8rpx;

}

.search-ico {
  width: 10%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.search-mc {
  width: 90%;
  height: 100%;
  font-size: 30rpx;
  color: #ccc;
  line-height: 60rpx;
}

.search-ico icon {
  position: relative;
  top: 6rpx;
}

.search-mc input {
  width: 100%;
  height: 60rpx;
}


.saoma {
  width:10%;
  height:60rpx;
  position:relative;
  background-color:#333;
  border-radius:100%;
  padding:8rpx;
  opacity:0.7;

}
.saoma:after{
  position: absolute;
  content: '';
  width: 30rpx;
  height: 30rpx;
  top: 50%;
  left: 50%;
  margin-left: -12rpx;
  margin-top: -12rpx;
  animation: sploosh3 3s cubic-bezier(0.165, 0.84, 0.44, 1);
	animation-iteration-count: infinite;
}
@keyframes sploosh3{0%{box-shadow:0 0 0 0 rgba(255, 255, 255, 0.5);background:rgba(255, 255, 255, 0.1)}100%{box-shadow:0 0 0 10px rgba(37, 143, 184, 0);}}

.saoma span {
  position:absolute;
  top:-10rpx;
  right:12rpx;
}

