}

.saoma {
  width: 10%;
  height: 60rpx;
  position: relative;
}
.saoma:after{
  position: absolute;
  content: '';
  width: 30rpx;
  height: 30rpx;
  top: 50%;
  left: 50%;
  margin-left: -10rpx;
  margin-top: -10rpx;
  animation: sploosh3 3s cubic-bezier(0.165, 0.84, 0.44, 1);
	animation-iteration-count: infinite;
}
@keyframes sploosh3{0%{box-shadow:0 0 0 0 rgba(0, 0, 0, 0.5);background:rgba(255, 255, 255, 0.1)}100%{box-shadow:0 0 0 10px rgba(37, 143, 184, 0);}}
.saoma span {
  position:absolute;
  top:-16rpx;

}

