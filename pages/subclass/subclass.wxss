.loading1 {
  width: 40%;
  text-align: right;
}

.loading2 {
  width: 60%;
  text-align: left;
}

.loadingmore {
  padding:30rpx 0 120rpx 0;
  text-align:center;
  font-size:30rpx;
  display:flex;
  flex-direction:row;
  flex-flow:row wrap;
  justify-content:center;
}

.loadingmore image {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: 2rpx;
  right: 8rpx;
}

.loadingmore image :before {
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}

.loadingmore.complete:before {
  display: none;
}

.move_up {
  animation: mymove 1s 1;
  animation-fill-mode: forwards;
}

@keyframes mymove {
  from {
    bottom: -110rpx;
  }

  to {
    bottom: 200rpx;
  }
}

.move_down {
  animation: mymove2 1s 1;
  animation-fill-mode: forwards;
}

@keyframes mymove2 {
  from {
    bottom: 200rpx;
  }

  to {
    bottom: -110rpx;
  }
}

