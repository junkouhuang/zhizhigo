.searchLoadingComplete {
  padding: 0 0 100rpx 0;
  text-align: center;
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  flex-flow: row wrap;
}

.searchLoadingComplete image {
  width: 40rpx;
  height: 40rpx;
}

.searchLoadingComplete text {
  position: relative;
  top: -6rpx;
  left: 16rpx;
}

.searchLoadingComplete image :before {
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}

.searchLoadingComplete.complete:before {
  display: none;
}

