.sel-accttype {
  background: rgba(0,0,0,0.6);
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 9;
}
.sel-accttype .sel-accttype-list {
  width: 80%;
  margin-left: 10%;
  background: #ffffff;
  height:860rpx;
  margin-top:-430rpx;
  border-radius:22rpx;
  position: absolute;
  top: 50%;
}
.sel-accttype .sel-accttype-list .sel-accttype-list-header {
  display: flex;
  flex-direction: row;
}
.sel-accttype .sel-accttype-list .sel-accttype-list-header view:nth-child(1){
  width: 50%;
  height: 90rpx;
  text-align:center;
  line-height:90rpx;
  font-size:28rpx;
  color:#ffffff;
  position: relative;
  background: #53a24f;
  border-top-left-radius:10px;
}
.tab::before {
  content: '';
  width:90rpx;
  height:4rpx;
  border-bottom: 6rpx solid #FEB511;
  position: absolute;
  bottom: 0;
}
.sel-accttype .sel-accttype-list .sel-accttype-list-header view:nth-child(2){
  width: 50%;
  height: 90rpx;
  text-align:center;
  line-height:90rpx;
  font-size:30rpx;
  color:#ffffff;
  background: #53a24f;
  position: relative;
  border-top-right-radius:20rpx;

}
.sel-accttype-list-body {
  width: 100%;
  height: 680rpx;
}
.sel-accttype-list-body-item {
  min-width:38%;
  float:left;
  height:60rpx;
  margin-left:6%;
  margin-top:14rpx;
  border-radius:6rpx;
}
.sel-accttype-list-body-item span{
  font-size:40rpx;
}
.accttypeHover {
  background-color:rgba(226, 212, 103, 0.3);
}
.sel-accttype-list-body-item image {
  width: 40rpx;
  height: 40rpx;
  position:relative;
  top:10rpx;
}
.sel-accttype-list-body-item text {
  font-size: 30rpx;
}
.sel-accttype-list-footer {
  width: 100%;
  display: flex;
  flex-direction: row;
  font-size:38rpx;
}
.sel-accttype-list-footer view{
  width: 50%;
  text-align: center;
  height:90rpx;
  line-height: 90rpx;
  background:#fff;
}
.sel-accttype-list-footer view:nth-child(1){
  border-top:2rpx solid #F2F2F2;
  border-bottom-left-radius:20rpx;
}
.sel-accttype-list-footer view:nth-child(2){
  background: #FEB511;
  color: #ffffff;
  border-top:2rpx solid #FEB511;
  border-bottom-right-radius:20rpx;
}

