page{
  background: #F0F4F5;
  height: 100%;
  position: relative;
  width: 100%;
  /**color: #939393;**/
  overflow: hidden;

}
.nav_left{
  display: inline-block;
  width: 30%;
  height: 100%;
  text-align: center;
  position:absolute;
  top:0;
  left:0;
}
.nav_left .nav_left_items{
  height: 70rpx;
  line-height: 70rpx;
  padding: 14rpx 0;
  border-bottom: 2rpx solid #F3F3F3;
  font-size: 32rpx;
}
.active{
  background: #f5f5f5;
  color:#53a24f;
  border-left:4rpx solid #53a24f;
  background:#ffffff;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 70%;
  height: 100%;
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
  
}
.navRightItems {
  display: flex;
  flex-direction: row;
  flex-flow: row nowrap;
  width: 100%;
  height: 100%;
}
.nav_right  .stageOne {
  width:50%;
  text-align:left;
  font-size:30rpx;
  margin:8rpx 0;
  float: left;  
}
.nav_right  .stageOne text{
  display:block;
  font-size:32rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
  padding:8rpx 0;

}
.nav_right  .stageTwo {
  width:100%;
  text-align:left;
  font-size:28rpx;
  font-weight: bold;
  margin:8rpx 0;
}
.nav_right_item{
    width: 50%;
    height: 100%;
    text-align: center;
    display:flex;
    flex-direction:row;
    flex-flow:row wrap;
}
.nav_right .nav_right_items{
  width: 100%;
  height: 100%;
  text-align: center;
  display:flex;
  flex-direction:row;
  flex-flow:row wrap;
}
.nav_right .nav_row_in {
  display:flex;
  flex-direction:column;
  width:100%;
  border-radius:6rpx;
  text-align:left;
}
.nav_right .nav_right_items image{
  width: 100rpx;
  height: 100rpx;
  margin:0 40rpx;
}
.nav_right .nav_right_items text{
  display: block;
  margin-top: 10rpx;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  padding:8rpx 10rpx;
}
.no_network {
  position: relative;
  width: 100%;
  height: 100%;
  background:#fff;
}

.no_network view {
  width:100%;
  text-align:center;
  font-size:30rpx;
  color:#ccc;
  position:absolute;
  bottom:50%;
}

.no_network text {
  padding:2rpx 8rpx;
  border:2rpx solid #ccc;
  border-radius:24rpx;
  width:66rpx;
  margin-left:8rpx;
}
.fl view {
  width:140rpx;
  height:72rpx;
  float:left;
  margin:5px;
  text-align:center;
  line-height:72rpx;
  font-size:32rpx;
  background-color:rgb(186, 225, 235);

}
.fl view:nth-child(5){
  background-color:#ccc(175, 191, 196);
}
.fl view:nth-child(6){
  background-color:#ccc(186, 225, 235);
}
